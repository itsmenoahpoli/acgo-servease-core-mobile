{
  "expo": {
    "name": "servease-consumer-app",
    "slug": "servease-consumer-app",
    "version": "1.0.0",
    "scheme": "servease-consumer-app",
    "platforms": [
      "ios",
      "android"
    ],
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.itsmenoahpoli.serveaseconsumerapp"
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "cafee59b-54a7-458b-9f2a-733c23bd45d4"
      }
    }
  }
}
